#include <stdio.h>
#include <stdlib.h>

//MODELO DE EXEMPLO

int main(){
   
   	//VariÃ¡veis que podem ser usadas
   	unsigned int x[200];
   	int y;
   	int z;
   	int k;
   	int w;
   	int j;
   	int i;
   	
   	//#########################
   	
   	for(i = 0; i <= 199; i++){
           x[i]= 0;
        }

   	while(1){

           printf("Digite um numero! ");
           scanf("%i", &z);

           if(z == -1){
              break;
           }

           else{
              w = z/32;
              x[w] = x[w] | (1 << (z - 32*w));
           }
        }

        z = 0;
        y = 0;
	
        while(y <= 199){

           for(z; z <= 31; z++){

              if(1 & (x[y] >> z)){
     
                 printf("%i \n", (z+(32*y)));
              }

           }
           y++;
           z = 0;
        }
   	//#########################
   
	return 0;  
}
